<odoo>
    <data>
        <record id="view_move_form_treatment_id" model="ir.ui.view">
            <field name="name">account.move.form.treatment</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="treatment_id" string="Treatment"/>
<!--                    <field name="treatment_code" string="Treatment" readonly="1"/>-->
                </xpath>

            </field>
        </record>
    </data>
</odoo>
