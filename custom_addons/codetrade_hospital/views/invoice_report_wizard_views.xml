<odoo>
    <record id="view_invoice_report_wizard_form" model="ir.ui.view">
        <field name="name">invoice.report.wizard.form</field>
        <field name="model">invoice.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Invoice Report">
                <group>
                    <field name="from_date" />
                    <field name="to_date" />
                </group>
                <footer>
                    <button string="Generate Report" type="object" name="generate_excel_report" class="btn-primary" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
